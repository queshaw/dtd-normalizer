<!ELEMENT attributes (string, tokenized, enumerations)>
<!ELEMENT string EMPTY>
<!ATTLIST string text CDATA #IMPLIED>

<!ELEMENT tokenized (id, tokens)>

<!ELEMENT enumerations EMPTY>
<!ATTLIST enumerations
          implied (a|b) #IMPLIED
          defaulted (x|y|z) 'y'
          fixed-enum (g|h|i) #FIXED 'i'>

<!ELEMENT id EMPTY>
<!ATTLIST id id ID #REQUIRED>

<!ELEMENT tokens (one, two, ref, refs)>
<!ATTLIST tokens
          cdata-default CDATA 'cdata default value'
          nmtoken-default NMTOKEN 'cdata-default-value'>

<!ENTITY % this 'INCLUDE'>
<!ENTITY % not-this 'IGNORE'>

<!ELEMENT idref EMPTY>
<!ELEMENT one EMPTY>
<![%this;[
<!ATTLIST one
          id ID #REQUIRED
          type NMTOKEN #IMPLIED>
]]>
<![%not-this;[
<!ATTLIST one
          bad-id ID #REQUIRED
          bad-type NMTOKEN #IMPLIED>
]]>

<!ELEMENT two EMPTY>
<!ATTLIST two
          id ID #REQUIRED
          types NMTOKENS #IMPLIED>

<!ELEMENT ref EMPTY>
<!ATTLIST ref href IDREF #REQUIRED>

<!ELEMENT refs EMPTY>
<!ATTLIST refs
          refs IDREFS #REQUIRED>

